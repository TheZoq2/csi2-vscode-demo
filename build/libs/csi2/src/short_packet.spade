enum ShortPacket {
    FrameStart,
    FrameEnd,
    Other
}

struct ShortPacketStream {
    s: Option<ShortPacket>
}

impl ShortPacketStream {
    fn is_frame_start(self) -> bool {
        match self.s {
            Some(ShortPacket::FrameStart) => true,
            _ => false
        }
    }

    fn is_frame_end(self) -> bool {
        match self.s {
            Some(ShortPacket::FrameEnd) => true,
            _ => false
        }
    }
}
